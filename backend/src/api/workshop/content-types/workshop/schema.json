    "venue": {
      "type": "string"
    },
    "banner_mobile": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "card_image": {
      "type": "media",
      "multiple": false,
